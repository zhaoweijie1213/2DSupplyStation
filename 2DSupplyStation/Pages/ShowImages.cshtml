@page
@model _2DSupplyStation.Pages.ShowImagesModel
@{
    ViewData["Title"] = "Show Images";
}

<div class="container">
    <div class="row">
        @foreach (var imagePath in Model.ImagePaths)
        {
            <div class="col-sm-6 col-md-4 col-lg-3">
                <div class="thumbnail">
                    <img src="@imagePath" alt="Image" class="img-fluid">
                </div>
            </div>
        }
    </div>
</div>
